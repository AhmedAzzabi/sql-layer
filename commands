
# Emit interface file

curl localhost:9000/api/direct/igen/test

# Emit an implementation class

curl localhost:9000/api/direct/cgen/test?table=customers

# Load a DirectModule

curl -X PUT "localhost:9000/api/direct/module/test?name=com.akiban.direct.DemoModule&url=file:/Users/peter/workspace/sandbox/bin/"

# Invoke

curl localhost:9000/api/direct/DemoModule/test

# Remove a DirectModule

curl -X DELETE "localhost:9000/api/direct/module/test?name=DemoModule&url=file:/Users/peter/workspace/sandbox/bin/"

