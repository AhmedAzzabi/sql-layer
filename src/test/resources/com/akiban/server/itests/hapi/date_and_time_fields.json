{

"setup" : {
    "processors_default" : ["scanrows"],

    "schema" : "test",

    "tables" : [
        ["years", "id int key", "y year"],
        ["dates", "id int key", "d date", "index(d)"],
        ["times", "id int key", "t time"],
        ["datetimes", "id int key", "dt datetime", "index(dt)"]
    ],
    
    "write_rows" : {
        "years"  : [
            [1, "2011"],
            [2, "1901"],
            [3, "0000"]
        ],

        "dates"  : [
            [1, "2011-04-07"],
            [2, "1901-01-01"],
            [3, "0000-00-00"]
        ],

        "times"  : [
        ],
        
        "datetimes"  : [
            [1, "2011-04-07 17:06:59"],
            [2, "1901-01-01 01:01:42"],
            [3, "0000-00-00 00:00:00"]
        ]
    }
},

"tests" : {
    "all_years" : {
        "get" : "test:years:id>0",
        "expect result" : {
            "@years" : [
                {"id" : 1, "y" : "2011"},
                {"id" : 2, "y" : "1901"},
                {"id" : 3, "y" : "0000"}
            ]
        }
    },


    "years_less" : {
        "get" : "test:years:y<'2000'",
        "expect result" : {
            "@years" : [
                {"id" : 2, "y" : "1901"},
                {"id" : 3, "y" : "0000"}
            ]
        }
    },

    "years_equal" : {
        "get" : "test:years:y='1901'",
        "expect result" : {
            "@years" : [
                {"id" : 2, "y" : "1901"}
            ]
        }
    },

    "years_greater" : {
        "get" : "test:years:y>'1950'",
        "expect result" : {
            "@years" : [
                {"id" : 1, "y" : "2011"}
            ]
        }
    },

    "all_dates" : {
        "get" : "test:dates:id>0",
        "expect result" : {
            "@dates" : [
                {"id" : 1, "d" : "2011-04-07"},
                {"id" : 2, "d" : "1901-01-01"},
                {"id" : 3, "d" : "0000-00-00"}
            ]
        }
    },

    "dates_less_year" : {
        "get" : "test:dates:d<'1950'",
        "expect result" : {
            "@dates" : [
                {"id" : 3, "d" : "0000-00-00"},
                {"id" : 2, "d" : "1901-01-01"}
            ]
        }
    },
    
    "dates_less_full" : {
        "get" : "test:dates:d<'1901-00-00'",
        "expect result" : {
            "@dates" : [
                {"id" : 3, "d" : "0000-00-00"}
            ]
        }
    },

    "dates_equal_year" : {
        "get" : "test:dates:d='2011'",
        "expect result" : {
            "@dates" : [
            ]
        }
    },

    "dates_equal_full" : {
        "get" : "test:dates:d='2011-04-07'",
        "expect result" : {
            "@dates" : [
                {"id" : 1, "d" : "2011-04-07"},
            ]
        }
    },

    "dates_greater_year" : {
        "get" : "test:dates:d>'1850'",
        "expect result" : {
            "@dates" : [
                {"id" : 2, "d" : "1901-01-01"},
                {"id" : 1, "d" : "2011-04-07"}
            ]
        }
    },

    "dates_greater_full" : {
        "get" : "test:dates:d>'1901-01-01'",
        "expect result" : {
            "@dates" : [
                {"id" : 1, "d" : "2011-04-07"}
            ]
        }
    },

    "all_times" : {
        "get"           : "test:times:id>0",
        "expect result" : {
            "@times" : [
                {"id" : 1, "t" : "17:06:59"},
                {"id" : 2, "t" : "01:01:42"},
                {"id" : 3, "t" : "00:00:00"}
            ]
        }
    },

    "all_datetimes" : {
        "get"           : "test:datetimes:id>0",
        "expect result" : {
            "@datetimes" : [
                {"id" : 1, "dt" : "2011-04-07 17:06:59"},
                {"id" : 2, "dt" : "1901-01-01 01:01:42"},
                {"id" : 3, "dt" : "0000-00-00 00:00:00"}
            ]
        }
    }
}

}
