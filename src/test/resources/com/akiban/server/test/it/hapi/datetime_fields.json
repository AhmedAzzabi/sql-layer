{

"setup" : {
    "processors_default" : ["scanrows"],

    "schema" : "test",

    "tables" : [
        ["datetimes", "id int not null primary key", "dt datetime", "index(dt)"]
    ],

    "write_rows" : {
        "datetimes"  : [
            [1, "2011-04-07 17:06:59"],
            [2, "1901-01-01 01:01:42"],
            [3, "0000-00-00 00:00:00"],
            [4, "2011-04-10 14:53:22"],
            [5, "0000-00-00 00:00:01"],
            [6, "2011-02-22 16:22:09"]
        ]
    }
},

"tests" : {
    "all_datetimes" : {
        "get" : "test:datetimes:id>0",
        "expect result" : {
            "@datetimes" : [
                {"id" : 1, "dt" : "2011-04-07 17:06:59"},
                {"id" : 2, "dt" : "1901-01-01 01:01:42"},
                {"id" : 3, "dt" : "0000-00-00 00:00:00"},
                {"id" : 4, "dt" : "2011-04-10 14:53:22"},
                {"id" : 5, "dt" : "0000-00-00 00:00:01"},
                {"id" : 6, "dt" : "2011-02-22 16:22:09"}
            ]
        }
    },
    
    "index_less_full" : {
        "get" : "test:datetimes:dt<'2000-01-01%2000%3A00%3A00'",
        "expect result" : {
            "@datetimes" : [
                {"id" : 3, "dt" : "0000-00-00 00:00:00"},
                {"id" : 5, "dt" : "0000-00-00 00:00:01"},
                {"id" : 2, "dt" : "1901-01-01 01:01:42"}
            ]
        }
    },

    "index_equal_full" : {
        "get" : "test:datetimes:dt='1901-01-01%2001%3A01%3A42'",
        "expect result" : {
            "@datetimes" : [
                {"id" : 2, "dt" : "1901-01-01 01:01:42"}
            ]
        }
    },

    "index_greater_full" : {
        "get" : "test:datetimes:dt>'0000-00-00%2000%3A00%3A00'",
        "expect result" : {
            "@datetimes" : [
                {"id" : 5, "dt" : "0000-00-00 00:00:01"},
                {"id" : 2, "dt" : "1901-01-01 01:01:42"},
                {"id" : 6, "dt" : "2011-02-22 16:22:09"},
                {"id" : 1, "dt" : "2011-04-07 17:06:59"},
                {"id" : 4, "dt" : "2011-04-10 14:53:22"}
            ]
        }
    }
}

}
