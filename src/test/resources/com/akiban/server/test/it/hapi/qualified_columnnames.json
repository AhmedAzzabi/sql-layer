{

"setup" : {
    "processors_default" : [ "scanrows" ],
    "schema" : "@schema",
    "tables" : [
        [ ":customers",
            "\"@id\" int not null primary key",
            "\":name\" varchar(32)",
            "index(\":name\")"
        ],

        [ "@orders",
            "id int not null primary key",
            "cid int",
            "GROUPING FOREIGN KEY (cid) REFERENCES \":customers\"(\"@id\")"
        ]
    ],

    "write_rows" : {
        ":customers"    : [ [11, "customer1"],
                            [12, "customer2"]
        ],
        "@orders"       : [ [21, 11]
        ]
    }
},

"tests" : {
    "child names" : {
        "get"           : "'%40schema':'%3Acustomers':'%40id'=11",
        "expect result" :
        {
            "@:customers" :
            [
                {
                    ":@id" : 11,
                    "::name" : "customer1",
                    "@@orders" :
                    [
                        {
                            "id" : 21,
                            "cid" : 11
                        }
                    ]
                }
            ]
        }
    }
}

}
