{

"setup" : {
    "processors_default" : ["scanrows"],

    "schema" : "test",

    "tables" : [
        ["floats", "id int key", "f1 float", "f2 float unsigned", "index(f1)", "index(f2)"]
    ],

    "write_rows" : {
        "floats"  : [
            [1, 42.12, 12.42],
            [2, 0.0, 3.14159],
            [3, -15.678, 1234560],
            [4, 9876540, 0.0],
            [5, null, 1986.1],
            [6, -345.765, null]
        ]
    }
},

"tests" : {
    "select_all" : {
        "get" : "test:floats:id>0",
        "expect result" : {
            "@floats" : [
                {"id" : 1, "f1" : 42.12,     "f2" : 12.42},
                {"id" : 2, "f1" : 0.0,       "f2" : 3.14159},
                {"id" : 3, "f1" : -15.678,   "f2" : 1234560},
                {"id" : 4, "f1" : 9876540, "f2" : 0.0},
                {"id" : 5, "f1" : null,      "f2" : 1986.1},
                {"id" : 6, "f1" : -345.765,  "f2" : null}
            ]
        }
    },

    "index_less_f1" : {
        "get" : "test:floats:f1<10",
        "expect result" : {
            "@floats" : [
                {"id" : 6, "f1" : -345.765,  "f2" : null},
                {"id" : 3, "f1" : -15.678,   "f2" : 1234560},
                {"id" : 2, "f1" : 0.0,       "f2" : 3.14159}
            ]
        }
    },

    "index_equal_f1" : {
        "get" : "test:floats:f1=0",
        "expect result" : {
            "@floats" : [
                {"id" : 2, "f1" : 0.0,       "f2" : 3.14159}
            ]
        }
    },

    "index_greater_f1" : {
        "get" : "test:floats:f1>42",
        "expect result" : {
            "@floats" : [
                {"id" : 1, "f1" : 42.12,     "f2" : 12.42},
                {"id" : 4, "f1" : 9876540, "f2" : 0.0}
            ]
        }
    },

    "index_less_f2" : {
        "get" : "test:floats:f2<50",
        "expect result" : {
            "@floats" : [
                {"id" : 4, "f1" : 9876540, "f2" : 0.0},
                {"id" : 2, "f1" : 0.0,       "f2" : 3.14159},
                {"id" : 1, "f1" : 42.12,     "f2" : 12.42}
            ]
        }
    },

    "index_equal_f2" : {
        "get" : "test:floats:f2=0",
        "expect result" : {
            "@floats" : [
                {"id" : 4, "f1" : 9876540, "f2" : 0.0}
            ]
        }
    },

    "index_greater_f2" : {
        "get" : "test:floats:f1>100",
        "expect result" : {
            "@floats" : [
                {"id" : 5, "f1" : null,      "f2" : 1986.1},
                {"id" : 3, "f1" : -15.678,   "f2" : 1234560}
            ]
        }
    }
}

}
