{

"comment" : "Can the unicode snowman really be a value?!",

"passing_default" : false,

"setup" : {
    "tables" : [
        [ "t1",
           "col char(1) key" ]
    ],
    "write_rows" : {
        "t1" : [
            ["☃"],
            ["Å"]
        ]
    }
},

"tests" : {
    "snowman" : {
        "get"           : "test:t1:col='%E2%98%83'",
        "expect result" :
        {
            "@t1" : [
                {
                    "col" : "\u2603"
                }
            ]
        }
    },
    "unicode fun" : {
        "get"           : "test:t1:col='%C3%85'",
        "expect result" :
        {
            "@t1" : [
                {
                    "col" : "\u00C5"
                }
            ]
        }
    }
}

}
