PhysicalSelect@1957a34[name, order_date, name]
  Map_NestedLoops
    Flatten_HKeyOrdered(test.customers INNER test.orders)
      AncestorLookup_Default(Index(test.orders.order_date[IndexColumn(order_date)]) -> [test.customers, test.orders])
        IndexScan_Default(Index(test.orders.order_date[IndexColumn(order_date)]) (>UnboundExpressions[CAST_DATE(Literal(2011-01-01))],<UnboundExpressions[Literal(NULL)]))
    Map_NestedLoops
      IndexScan_Default(Index(test.parent.name[IndexColumn(name)]) (>=UnboundExpressions[Bound(0,Field(1))],<=UnboundExpressions[Bound(0,Field(1))]))
      project([Bound(0,Field(1)), Bound(0,Field(4)), Field(2)])
        Select_HKeyOrdered(test.child, Field(2) != Literal(Smith))
          Select_HKeyOrdered(test.child, Field(2) == Bound(1,Field(0)))
            Filter_Default([test.child])
              GroupScan_Default(full scan on _akiban_parent)
