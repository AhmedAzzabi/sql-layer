PhysicalSelect[cid:int[INT]]
  Map_NestedLoops(test.parent)
    AncestorLookup_Default(test._akiban_parent -> test.parent)
      IndexScan_Default(Index(test.parent.PRIMARY[IndexColumn(id)]), id > 1)
    Project_Default(customers.cid)
      Product_NestedLoops(test.customers - test.items x test.customers - test.addresses)
        Flatten_HKeyOrdered(test.customers INNER test.items)
          Select_HKeyOrdered(items.quan > 100)
            AncestorLookup_Default(test._akiban_customers -> test.customers, test.items)
              IndexScan_Default(Index(.customers.cname_and_sku[IndexColumn(name), IndexColumn(sku)]), name = parent.name, sku = 1234, cid, oid, iid)
        Select_HKeyOrdered(addresses.city != customers.name)
          Filter_Default(test.customers - test.addresses)
            Flatten_HKeyOrdered(test.customers INNER test.addresses)
              Select_HKeyOrdered(Bound(0,Field(2)) == addresses.state)
                BranchLookup_Nested(Binding at 1 -> test.addresses (via test.customers))