PhysicalSelect[name:varchar(32)[VARCHAR], quan:int[INT]]
  Sort_General(Field(1) DESC)
    Map_NestedLoops()
      ValuesScan_Default(['MA'], ['NY'], ['VT'])
      Project_Default(customers.name, items.quan)
        Filter_Default(customers - orders - items)
          Flatten_HKeyOrdered(customers - orders INNER items)
            Flatten_HKeyOrdered(customers INNER orders)
              BranchLookup_Default(customers -> orders)
                AncestorLookup_Default(Index(addresses.state) -> customers)
                  IndexScan_Default(Index(addresses.state), state = Bound(0, 0))