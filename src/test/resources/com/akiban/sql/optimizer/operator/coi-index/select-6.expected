              BranchLookup_Default(_akiban_customers Index(user.orders.order_date[IndexColumn(order_date)]) -> user.orders limit NO LIMIT)
                IndexScan_Default(Index(user.orders.order_date[IndexColumn(order_date)]) (,) reverse)