PhysicalSelect[name:varchar(32)[VARCHAR], order_date:date[DATE]]
  Project_Default(customers.name, orders.order_date)
    Filter_Default(test.customers - test.orders - test.items)
      Flatten_HKeyOrdered(test.customers - test.orders INNER test.items)
        Flatten_HKeyOrdered(test.customers INNER test.orders)
          AncestorLookup_Default(test._akiban_customers -> test.customers)
            Select_HKeyOrdered(items.quan > 100)
              BranchLookup_Default(test._akiban_customers -> test.orders, test.items (via test.orders))
                IndexScan_Default(Index(test.orders.order_date[IndexColumn(order_date)]), order_date, cid, oid)