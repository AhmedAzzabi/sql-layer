PhysicalSelect[name:varchar(32)[VARCHAR], order_date:date[DATE], state:char(2)[VARCHAR]]
  Project_Default(customers.name, orders.order_date, addresses.state)
    Map_NestedLoops()
      Filter_Default(customers - addresses)
        Flatten_HKeyOrdered(customers INNER addresses)
          BranchLookup_Default(Index(customers.name) -> customers)
            IndexScan_Default(Index(customers.name), name = 'IBM')
      Product_Nested(customers - addresses x customers - orders)
        Filter_Default(customers - orders)
          Flatten_HKeyOrdered(customers INNER orders)
            BranchLookup_Nested(customers -> orders)