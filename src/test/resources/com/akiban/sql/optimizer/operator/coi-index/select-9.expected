PhysicalSelect[name:varchar(32)[VARCHAR]]
  Project_Default(customers.name)
    Filter_Default(test.customers - test.items)
      Flatten_HKeyOrdered(test.customers INNER test.items)
        AncestorLookup_Default(test._akiban_customers -> test.customers)
          BranchLookup_Default(test._akiban_customers -> test.items (via test.orders))
            IndexScan_Default(Index(test.orders.order_date[IndexColumn(order_date)]), order_date = 2011-03-01, cid, oid)