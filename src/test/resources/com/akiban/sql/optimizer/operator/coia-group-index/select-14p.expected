PhysicalSelect[VARCHAR(32) CHARACTER SET utf8 COLLATE utf8_bin, VARCHAR(32) CHARACTER SET utf8 COLLATE utf8_bin, VARCHAR(32) CHARACTER SET utf8 COLLATE utf8_bin][order_date:date[DATE], sku:varchar(32)[VARCHAR], quan:int[INT]]
  Map_NestedLoops()
    Sort_InsertionLimited(Field(0) ASC, LIMIT 4, SUPPRESS DUPLICATES)
      ValuesScan_Default(['1234'], [$1], [$2], [$3])
    Project_Default(orders.order_date, items.sku, items.quan)
      Flatten_HKeyOrdered(orders INNER items)
        AncestorLookup_Default(Index(.customers.cname_and_sku) -> orders, items)
          IndexScan_Default(Index(.customers.cname_and_sku), customers.name = 'Smith', items.sku = Bound(3, 0))