PhysicalSelect[name:varchar(32)[VARCHAR]]
  project(Field(1))
    Filter_Default([flatten(test.customers, test.orders)])
      Flatten_HKeyOrdered(test.customers INNER test.orders)
        Select_HKeyOrdered(Field(2) == "2011-03-02")
          BranchLookup_Default(_akiban_customers test.customers -> test.orders limit NO LIMIT)
            AncestorLookup_Default(Index(test.orders.order_date[IndexColumn(order_date)]) -> [test.customers])
              IndexScan_Default(Index(test.orders.order_date[IndexColumn(order_date)]) (>=UnboundExpressions[Literal(2011-03-01)],<=UnboundExpressions[Literal(2011-03-01)]))