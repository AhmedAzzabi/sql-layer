PhysicalSelect[name:varchar(32)[VARCHAR]]
  project([Field(1)])
    Filter_Default([flatten(user.customers, user.orders)])
      Flatten_HKeyOrdered(user.customers INNER user.orders)
        Select_HKeyOrdered(user.orders, Field(2) == Literal(2011-03-02))
          BranchLookup_Default(_akiban_customers user.customers -> user.orders limit NO LIMIT)
            AncestorLookup_Default(Index(user.orders.order_date[IndexColumn(order_date)]) -> [user.customers])
              IndexScan_Default(Index(user.orders.order_date[IndexColumn(order_date)]) (>=UnboundExpressions[Literal(2011-03-01)],<=UnboundExpressions[Literal(2011-03-01)]))