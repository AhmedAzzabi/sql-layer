PhysicalSelect[name:varchar(32)[VARCHAR], order_date:DATE NOT NULL]
  Map_NestedLoops
    IndexScan_Default(Index(user.customers.name[IndexColumn(name)]) (,))
    project([Bound(0,Field(0)), Field(1)])
      IfEmpty_Default(Literal(NULL), Literal(NULL))
        Select_HKeyOrdered(project([Field(1), Field(0)]), Bound(0,Field(1)) == Field(0))
          project([Field(1), Field(0)])
            IndexScan_Default(Index(user.orders.order_date[IndexColumn(order_date)]) (>UnboundExpressions[Literal(NULL)],<UnboundExpressions[Literal(2011-01-01)]))