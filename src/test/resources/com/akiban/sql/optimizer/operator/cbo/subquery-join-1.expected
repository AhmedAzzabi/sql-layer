PhysicalSelect[name:varchar(32)[VARCHAR], order_date:DATE NOT NULL]
  Map_NestedLoops
    Filter_Default([user.customers])
      GroupScan_Default(full scan on _akiban_customers)
    project([Bound(0,Field(1)), Field(1)])
      IfEmpty_Default(Literal(NULL), Literal(NULL))
        project([Field(1), Field(2)])
          Select_HKeyOrdered(user.orders, Field(2) < Literal(2011-01-01))
            AncestorLookup_Default(Index(user.orders.__akiban_fk_0[IndexColumn(cid)]) -> [user.orders])
              IndexScan_Default(Index(user.orders.__akiban_fk_0[IndexColumn(cid)]) (>=UnboundExpressions[Bound(0,Field(0))],<=UnboundExpressions[Bound(0,Field(0))]))